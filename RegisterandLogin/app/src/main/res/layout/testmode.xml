<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ankitbulchandani.registerandlogin.TestMode">

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/test_ages"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/ages128"
        android:onClick="OntestAge"
        />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/button3"
        android:layout_centerHorizontal="true"
        android:background="@drawable/average128" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button3"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/percentage128" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button4"
        android:layout_below="@+id/test_ages"
        android:layout_alignParentStart="true"
        android:layout_marginTop="59dp"
        android:background="@drawable/area" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button5"
        android:layout_alignTop="@+id/button4"
        android:layout_alignStart="@+id/button2"
        android:background="@drawable/logarithm" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button6"
        android:layout_alignTop="@+id/button5"
        android:layout_alignStart="@+id/button3"
        android:background="@drawable/time128" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button7"
        android:layout_below="@+id/button4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="59dp"
        android:background="@drawable/icon123" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button8"
        android:layout_alignTop="@+id/button7"
        android:layout_alignStart="@+id/button5"
        android:background="@drawable/ratiopropotion" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/button9"
        android:layout_alignTop="@+id/button8"
        android:layout_alignParentEnd="true"
        android:background="@drawable/squareroot" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ages"
        android:gravity="center"
        android:id="@+id/textView2"
        android:layout_below="@+id/test_ages"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/button5"
        android:layout_alignEnd="@+id/test_ages" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Average"
        android:gravity="center"
        android:id="@+id/textView3"
        android:layout_below="@+id/button2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignEnd="@+id/button2"
        android:layout_alignStart="@+id/button2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Percentage"
        android:gravity="center"
        android:id="@+id/textView4"
        android:layout_below="@+id/button3"
        android:layout_alignEnd="@+id/button3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignStart="@+id/button3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Area &amp; Volume"
        android:gravity="center"
        android:id="@+id/textView5"
        android:layout_below="@+id/button4"
        android:layout_above="@+id/button8"
        android:layout_alignEnd="@+id/button4"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Logarithms"
        android:gravity="center"
        android:id="@+id/textView6"
        android:layout_below="@+id/button5"
        android:layout_above="@+id/button8"
        android:layout_alignEnd="@+id/button5"
        android:layout_alignStart="@+id/button5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Clock"
        android:gravity="center"
        android:id="@+id/textView7"
        android:layout_below="@+id/button5"
        android:layout_above="@+id/button8"
        android:layout_alignEnd="@+id/button6"
        android:layout_alignStart="@+id/button6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Profit &amp; Loss"
        android:gravity="center"
        android:id="@+id/textView8"
        android:layout_below="@+id/button7"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/button7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ratio &amp; Propotion"
        android:gravity="center"
        android:id="@+id/textView9"
        android:layout_alignTop="@+id/textView8"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/button8"
        android:layout_alignStart="@+id/button8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Square Root &amp; Cube Root"
        android:gravity="center"
        android:id="@+id/textView10"
        android:layout_alignTop="@+id/textView9"
        android:layout_alignStart="@+id/button9"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/button9" />


</RelativeLayout>
